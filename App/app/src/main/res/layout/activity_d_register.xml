<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register_background_d"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".register">

    <TextView
        android:id="@+id/app_textView_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/app_logo"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/pass_editText_d"
        style="@style/Edit_Text"
        android:layout_width="315dp"
        android:layout_height="74dp"
        android:layout_marginTop="30dp"
        android:autofillHints="@string/app_password"
        android:hint="@string/app_password"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/mail_editText_d"
        app:layout_constraintStart_toStartOf="@+id/mail_editText_d"
        app:layout_constraintTop_toBottomOf="@+id/mail_editText_d" />

    <EditText
        android:id="@+id/confirm_editText_d"
        style="@style/Edit_Text"
        android:layout_width="315dp"
        android:layout_height="74dp"
        android:layout_marginTop="30dp"
        android:autofillHints="@string/app_user_confirm"
        android:hint="@string/app_user_confirm"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/pass_editText_d"
        app:layout_constraintStart_toStartOf="@+id/pass_editText_d"
        app:layout_constraintTop_toBottomOf="@+id/pass_editText_d" />

    <EditText
        android:id="@+id/mail_editText_d"
        style="@style/Edit_Text"
        android:layout_width="315dp"
        android:layout_height="74dp"
        android:layout_marginTop="30dp"
        android:autofillHints="@string/app_user_email"
        android:hint="@string/app_user_email"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/user_editTex_d"
        app:layout_constraintStart_toStartOf="@+id/user_editTex_d"
        app:layout_constraintTop_toBottomOf="@+id/user_editTex_d" />

    <EditText
        android:id="@+id/user_editTex_d"
        style="@style/Edit_Text"
        android:layout_width="315dp"
        android:layout_height="74dp"
        android:layout_marginTop="50dp"
        android:autofillHints="@string/app_contentDescription1"
        android:hint="@string/app_user"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/create_textView_d"
        app:layout_constraintStart_toStartOf="@+id/create_textView_d"
        app:layout_constraintTop_toBottomOf="@+id/create_textView_d" />

    <Button
        android:id="@+id/register_button_d"
        android:layout_width="320dp"
        android:layout_height="60dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="23dp"
        android:background="@drawable/buttonshape"
        android:contentDescription="@string/app_button_register"
        android:fontFamily="@font/allerta_stencil"
        android:onClick="startRegister2"
        android:shadowColor="#FCE4EC"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        android:text="@string/app_button_register"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:backgroundTint="#1e1e1e"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/confirm_editText_d" />

    <TextView
        android:id="@+id/create_textView_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:fontFamily="sans-serif-medium"
        android:text="@string/app_text1"
        android:textAlignment="center"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/app_textView_d" />

    <ImageView
        android:id="@+id/register_imageView6_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:contentDescription="@string/app_contentDescription1"
        app:layout_constraintBottom_toBottomOf="@+id/user_editTex_d"
        app:layout_constraintStart_toStartOf="@+id/user_editTex_d"
        app:layout_constraintTop_toTopOf="@+id/user_editTex_d"
        app:srcCompat="@drawable/person_icon" />

    <ImageView
        android:id="@+id/register_imageView7_d"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="30dp"
        android:contentDescription="@string/app_user_email"
        app:layout_constraintBottom_toBottomOf="@+id/mail_editText_d"
        app:layout_constraintStart_toStartOf="@+id/mail_editText_d"
        app:layout_constraintTop_toTopOf="@+id/mail_editText_d"
        app:srcCompat="@drawable/mail_icon" />

    <ImageView
        android:id="@+id/register_imageView8_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:contentDescription="@string/app_contentDescription"
        app:layout_constraintBottom_toBottomOf="@+id/pass_editText_d"
        app:layout_constraintStart_toStartOf="@+id/pass_editText_d"
        app:layout_constraintTop_toTopOf="@+id/pass_editText_d"
        app:srcCompat="@drawable/lock_icon" />

    <ImageView
        android:id="@+id/register_imageView10_d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:contentDescription="@string/app_user_confirm"
        app:layout_constraintBottom_toBottomOf="@+id/confirm_editText_d"
        app:layout_constraintStart_toStartOf="@+id/confirm_editText_d"
        app:layout_constraintTop_toTopOf="@+id/confirm_editText_d"
        app:srcCompat="@drawable/lock_icon" />

    <View
        android:id="@+id/register_divider11_d"
        android:layout_width="wrap_content"
        android:layout_height="2dp"
        android:layout_marginTop="7dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/app_textView_d" />

</androidx.constraintlayout.widget.ConstraintLayout>