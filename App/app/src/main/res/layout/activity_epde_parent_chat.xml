<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parent_chat_background_epde"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".epd_parent_chat">

    <TextView
        android:id="@+id/chat_textView_epde"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pacifico_regular"
        android:text="@string/Jane_Doe"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/chat_divider45_epde"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="9dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView_epde" />

    <ImageView
        android:id="@+id/chat_imageView_epde"
        android:layout_width="63dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:contentDescription="@string/contentDescription_sitter_image"
        app:layout_constraintStart_toStartOf="@+id/chat_divider45_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider45_epde"
        app:srcCompat="@drawable/sitter" />

    <TextView
        android:id="@+id/chat_textView2_epde"
        android:layout_width="211dp"
        android:layout_height="59dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:text="@string/Hi_I_cannot_wait"
        app:layout_constraintStart_toEndOf="@+id/chat_imageView_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider45_epde" />

    <TextView
        android:id="@+id/chat_textView3_epde"
        android:layout_width="241dp"
        android:layout_height="58dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="30dp"
        android:text="@string/Hey_there"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView2_epde" />

    <ImageView
        android:id="@+id/chat_imageView2_epde"
        android:layout_width="63dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:contentDescription="@string/contentDescription_sitter_image"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView3_epde"
        app:srcCompat="@drawable/sitter" />

    <TextView
        android:id="@+id/chat_textView4_epde"
        android:layout_width="213dp"
        android:layout_height="54dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:text="@string/i_love"
        app:layout_constraintStart_toEndOf="@+id/chat_imageView2_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView3_epde" />

    <View
        android:id="@+id/chat_divider46_epde"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="360dp"
        android:layout_marginBottom="120dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView4_epde" />

    <ImageButton
        android:id="@+id/chat_home_imageButton_epde"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/home_icon"
        android:onClick="startHomeP4"
        app:layout_constraintStart_toStartOf="@+id/chat_divider46_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider46_epde"
        app:srcCompat="@drawable/home_icon" />

    <ImageButton
        android:id="@+id/chat_request_imageButton2_epde"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/requests"
        android:onClick="startRequestP"
        app:layout_constraintStart_toEndOf="@+id/chat_home_imageButton_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider46_epde"
        app:srcCompat="@drawable/event_icon" />

    <ImageButton
        android:id="@+id/chat_messages_imageButton3_epde"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:contentDescription="@string/email_icon"
        android:onClick="startMessagesP6"
        app:layout_constraintStart_toEndOf="@+id/chat_request_imageButton2_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider46_epde"
        app:srcCompat="@drawable/mail_icon" />

    <ImageButton
        android:id="@+id/chat_profile_imageButton5_epde"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:contentDescription="@string/person_icon"
        android:onClick="startProfileP4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/chat_messages_imageButton3_epde"
        app:layout_constraintTop_toBottomOf="@+id/chat_divider46_epde"
        app:srcCompat="@drawable/person_icon" />

    <LinearLayout
        android:id="@+id/inputLayout_epde"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="32dp"
        android:orientation="horizontal"
        android:padding="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/chat_divider46_epde"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <EditText
            android:id="@+id/type_editTextMessage_epde"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:autofillHints="@string/Type_your_message"
            android:hint="@string/Type_your_message"
            android:inputType="text|textAutoComplete" />

        <Button
            android:id="@+id/send_button_epde"
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:background="@drawable/buttonshape"
            android:backgroundTint="#1e1e1e"
            android:fontFamily="@font/allerta_stencil"
            android:shadowColor="#FCE4EC"
            android:shadowDx="2"

            android:shadowDy="2"
            android:text="@string/Send"
            android:textColor="#FFFFFF"
            android:textStyle="bold"
            app:backgroundTint="#1e1e1e" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>