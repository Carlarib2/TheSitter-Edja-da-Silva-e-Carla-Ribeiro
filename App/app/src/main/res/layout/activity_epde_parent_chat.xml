<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".epd_parent_chat">

    <TextView
        android:id="@+id/chat_textView_ep"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pacifico_regular"
        android:text="Jane Doe"
        android:textSize="34dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider45"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="9dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView_ep" />

    <ImageView
        android:id="@+id/chat_imageView_ep"
        android:layout_width="63dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@+id/divider45"
        app:layout_constraintTop_toBottomOf="@+id/divider45"
        app:srcCompat="@drawable/sitter" />

    <TextView
        android:id="@+id/chat_textView2_ep"
        android:layout_width="211dp"
        android:layout_height="59dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:text="Hi! I can't wait to meet your beautiful children. Is there anything you'd like to share more?"
        app:layout_constraintStart_toEndOf="@+id/chat_imageView_ep"
        app:layout_constraintTop_toBottomOf="@+id/divider45" />

    <TextView
        android:id="@+id/chat_textView3_ep"
        android:layout_width="241dp"
        android:layout_height="58dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="30dp"
        android:text="Hey there! Ava loves reading. If yould read for her, it'd be perfect."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView2_ep" />

    <ImageView
        android:id="@+id/chat_imageView2_ep"
        android:layout_width="63dp"
        android:layout_height="60dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView3_ep"
        app:srcCompat="@drawable/sitter" />

    <TextView
        android:id="@+id/chat_textView4_ep"
        android:layout_width="213dp"
        android:layout_height="54dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="40dp"
        android:text="I love reading! Thanks for letting me know."
        app:layout_constraintStart_toEndOf="@+id/chat_imageView2_ep"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView3_ep" />

    <View
        android:id="@+id/divider46"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="320dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chat_textView4_ep" />

    <ImageButton
        android:id="@+id/chat_home_imageButton_ep"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:onClick="startHomeP4"
        app:layout_constraintStart_toStartOf="@+id/divider46"
        app:layout_constraintTop_toBottomOf="@+id/divider46"
        app:srcCompat="@drawable/home_icon" />

    <ImageButton
        android:id="@+id/chat_request_imageButton2_ep"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:onClick="startRequestP"
        app:layout_constraintStart_toEndOf="@+id/chat_home_imageButton_ep"
        app:layout_constraintTop_toBottomOf="@+id/divider46"
        app:srcCompat="@drawable/event_icon" />

    <ImageButton
        android:id="@+id/chat_messages_imageButton3_ep"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:onClick="startMessagesP6"
        app:layout_constraintStart_toEndOf="@+id/chat_request_imageButton2_ep"
        app:layout_constraintTop_toBottomOf="@+id/divider46"
        app:srcCompat="@drawable/mail_icon" />

    <ImageButton
        android:id="@+id/chat_profile_imageButton5_ep"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:onClick="startProfileP4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/chat_messages_imageButton3_ep"
        app:layout_constraintTop_toBottomOf="@+id/divider46"
        app:srcCompat="@drawable/person_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>