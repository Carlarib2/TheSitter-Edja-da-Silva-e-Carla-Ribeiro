<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".my_rate">

    <TextView
        android:id="@+id/text_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pacifico_regular"
        android:text="Set your hourly rate"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider32"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginTop="7dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_message" />

    <View
        android:id="@+id/divider39"
        android:layout_width="409dp"
        android:layout_height="3dp"
        android:layout_marginTop="100dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider32" />

    <View
        android:id="@+id/divider40"
        android:layout_width="409dp"
        android:layout_height="3dp"
        android:layout_marginTop="70dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider41" />

    <View
        android:id="@+id/divider41"
        android:layout_width="409dp"
        android:layout_height="3dp"
        android:layout_marginTop="70dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider39" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="25dp"
        android:text="Set your base hourly pay"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/divider39"
        app:layout_constraintTop_toBottomOf="@+id/divider39" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="96dp"
        android:layout_height="60dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:hint="$"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/divider39"
        app:layout_constraintTop_toBottomOf="@+id/divider39" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="25dp"
        android:text="Parents are charged (hourly)"
        android:textColor="#8C8686"
        app:layout_constraintStart_toStartOf="@+id/divider41"
        app:layout_constraintTop_toBottomOf="@+id/divider41" />

    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="96dp"
        android:layout_height="60dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:hint="$"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toEndOf="@+id/divider41"
        app:layout_constraintTop_toBottomOf="@+id/divider41" />

    <Button
        android:id="@+id/register_button4"
        android:layout_width="341dp"
        android:layout_height="60dp"
        android:layout_marginTop="325dp"
        android:background="@drawable/buttonshape"
        android:contentDescription="@string/Save"
        android:fontFamily="@font/allerta_stencil"
        android:onClick="startSave2"
        android:shadowColor="#FCE4EC"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        android:text="@string/Save"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        app:backgroundTint="#1e1e1e"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider40" />

    <ImageButton
        android:id="@+id/imageButton38"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:onClick="startProfile2"
        android:src="@drawable/person_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton37"
        app:layout_constraintTop_toBottomOf="@+id/divider42" />

    <ImageButton
        android:id="@+id/imageButton37"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:onClick="startMessages3"
        android:src="@drawable/mail_icon"
        app:layout_constraintStart_toEndOf="@+id/imageButton36"
        app:layout_constraintTop_toBottomOf="@+id/divider42" />

    <ImageButton
        android:id="@+id/imageButton36"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:onClick="startRequests2"
        android:src="@drawable/event_icon"
        app:layout_constraintStart_toEndOf="@+id/imageButton35"
        app:layout_constraintTop_toBottomOf="@+id/divider42" />

    <ImageButton
        android:id="@+id/imageButton35"
        android:layout_width="64dp"
        android:layout_height="52dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/home_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/divider42" />

    <View
        android:id="@+id/divider42"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/register_button4" />

</androidx.constraintlayout.widget.ConstraintLayout>