<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".parent_create_family_profile">

    <EditText
        android:id="@+id/editText28"
        style="@style/Edit_Text_Profile"
        android:layout_width="384dp"
        android:layout_height="65dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/Address"
        android:firstBaselineToTopHeight="5dp"
        android:hint="Allergies"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText13"
        app:layout_constraintStart_toStartOf="@+id/editText13"
        app:layout_constraintTop_toBottomOf="@+id/editText13" />

    <EditText
        android:id="@+id/editText29"
        style="@style/Edit_Text_Profile"
        android:layout_width="382dp"
        android:layout_height="180dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/Address"
        android:firstBaselineToTopHeight="5dp"
        android:hint="Important notes"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText28"
        app:layout_constraintStart_toStartOf="@+id/editText28"
        app:layout_constraintTop_toBottomOf="@+id/editText28" />

    <EditText
        android:id="@+id/editText14"
        style="@style/Edit_Text_Profile"
        android:layout_width="384dp"
        android:layout_height="65dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/Address"
        android:firstBaselineToTopHeight="5dp"
        android:hint="Birthday"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText27"
        app:layout_constraintStart_toStartOf="@+id/editText27"
        app:layout_constraintTop_toBottomOf="@+id/editText27" />

    <EditText
        android:id="@+id/editText13"
        style="@style/Edit_Text_Profile"
        android:layout_width="384dp"
        android:layout_height="65dp"
        android:layout_marginTop="20dp"
        android:autofillHints="@string/Address"
        android:firstBaselineToTopHeight="5dp"
        android:hint="school *"
        android:inputType="text"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText14"
        app:layout_constraintStart_toStartOf="@+id/editText14"
        app:layout_constraintTop_toBottomOf="@+id/editText14" />

    <EditText
        android:id="@+id/editText27"
        style="@style/Edit_Text_Profile"
        android:layout_width="384dp"
        android:layout_height="65dp"
        android:layout_marginTop="50dp"
        android:autofillHints="@string/Address"
        android:firstBaselineToTopHeight="5dp"
        android:hint="Name"
        android:inputType="text"
        android:paddingLeft="50dp"
        android:textAllCaps="false"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView49" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/allerta_stencil"
        android:text="Create Family Profile"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:onClick="startProfile2"
        android:src="@drawable/person_icon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton29"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <ImageButton
        android:id="@+id/imageButton29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:onClick="startMessages3"
        android:src="@drawable/mail_icon"
        app:layout_constraintStart_toEndOf="@+id/imageButton28"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <ImageButton
        android:id="@+id/imageButton28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="30dp"
        android:onClick="startRequests2"
        android:src="@drawable/event_icon"
        app:layout_constraintStart_toEndOf="@+id/imageButton27"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <ImageButton
        android:id="@+id/imageButton27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/home_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button6" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:background="@drawable/buttonshape"
        android:contentDescription="@string/Save"
        android:shadowColor="#D78FFE"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        android:text="save"
        android:textColor="#1a436e"
        android:textStyle="bold"
        app:backgroundTint="#FFFFFF"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="772dp" />
</androidx.constraintlayout.widget.ConstraintLayout>