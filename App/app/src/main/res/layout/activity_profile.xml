<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    android:contentDescription="@string/description_icon"
    tools:context=".profile">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/pacifico_regular"
        android:text="Profile"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText24"
        style="@style/Edit_Text_Profile"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/day"
        android:hint="@string/day"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/editText23"
        style="@style/Edit_Text_Profile"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/other"
        android:hint="@string/other"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/editText22"
        style="@style/Edit_Text_Profile"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/female"
        android:hint="@string/female"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/editText25"
        style="@style/Edit_Text_Profile"
        android:layout_width="120dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/month"
        android:hint="@string/month"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/editText24"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/editText26"
        style="@style/Edit_Text_Profile"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/year"
        android:hint="@string/year"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView3"
        app:layout_constraintStart_toEndOf="@+id/editText25"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <EditText
        android:id="@+id/editText21"
        style="@style/Edit_Text_Profile"
        android:layout_width="70dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/male"
        android:hint="@string/male"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/editText23"
        app:layout_constraintStart_toEndOf="@+id/editText22"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <EditText
        android:id="@+id/editText20"
        style="@style/Edit_Text_Profile"
        android:layout_width="325dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/mobile"
        android:hint="@string/mobile"
        android:inputType="number"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText23"
        app:layout_constraintStart_toStartOf="@+id/editText22"
        app:layout_constraintTop_toBottomOf="@+id/editText21" />

    <EditText
        android:id="@+id/editText19"
        style="@style/Edit_Text_Profile"
        android:layout_width="325dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/Address"
        android:hint="@string/Address"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText16"
        app:layout_constraintStart_toStartOf="@+id/editText16"
        app:layout_constraintTop_toBottomOf="@+id/editText16" />

    <EditText
        android:id="@+id/editText18"
        style="@style/Edit_Text_Profile"
        android:layout_width="325dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/Experience"
        android:hint="@string/Experience"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText20"
        app:layout_constraintStart_toStartOf="@+id/editText20"
        app:layout_constraintTop_toBottomOf="@+id/editText20" />

    <EditText
        android:id="@+id/editText17"
        style="@style/Edit_Text_Profile"
        android:layout_width="0dp"
        android:layout_height="45dp"
        android:layout_marginTop="16dp"
        android:autofillHints="@string/About_me"
        android:hint="@string/About_me"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText18"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editText18"
        app:layout_constraintTop_toBottomOf="@+id/editText18" />

    <EditText
        android:id="@+id/editText16"
        style="@style/Edit_Text_Profile"
        android:layout_width="325dp"
        android:layout_height="45dp"
        android:layout_marginTop="15dp"
        android:autofillHints="@string/Education"
        android:hint="@string/Education"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/editText15"
        app:layout_constraintStart_toStartOf="@+id/editText15"
        app:layout_constraintTop_toBottomOf="@+id/editText15" />

    <EditText
        android:id="@+id/editText15"
        style="@style/Edit_Text_Profile"
        android:layout_width="325dp"
        android:layout_height="45dp"
        android:layout_marginTop="50dp"
        android:autofillHints="@string/full_name"
        android:hint="@string/full_name"
        android:inputType="text"
        android:textAlignment="center"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="@+id/divider15"
        app:layout_constraintStart_toStartOf="@+id/divider15"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <Button
        android:id="@+id/register_button3"
        android:layout_width="341dp"
        android:layout_height="60dp"
        android:background="@drawable/buttonshape"
        android:contentDescription="@string/Save"
        android:fontFamily="@font/allerta_stencil"
        android:onClick="startSave"
        android:shadowColor="#F3E5F5"
        android:shadowDx="2"
        android:shadowDy="2"
        android:shadowRadius="2"
        android:text="@string/Save"
        android:textColor="@color/white"
        android:textStyle="bold"
        app:backgroundTint="#1e1e1e"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editText17"
        app:layout_constraintStart_toStartOf="@+id/editText17"
        app:layout_constraintTop_toBottomOf="@+id/editText17" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="@string/Date_of_Birth"
        android:textAlignment="viewStart"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/editText19"
        app:layout_constraintStart_toStartOf="@+id/editText19"
        app:layout_constraintTop_toBottomOf="@+id/editText19" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="@string/Gender"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/editText26"
        app:layout_constraintStart_toStartOf="@+id/editText24"
        app:layout_constraintTop_toBottomOf="@+id/editText25" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/person_icon"
        app:layout_constraintBottom_toBottomOf="@+id/editText15"
        app:layout_constraintEnd_toEndOf="@+id/editText15"
        app:layout_constraintStart_toStartOf="@+id/editText15"
        app:layout_constraintTop_toTopOf="@+id/editText15"
        app:srcCompat="@drawable/person_icon" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/home_icon"
        app:layout_constraintBottom_toBottomOf="@+id/editText19"
        app:layout_constraintEnd_toEndOf="@+id/editText19"
        app:layout_constraintStart_toStartOf="@+id/editText19"
        app:layout_constraintTop_toTopOf="@+id/editText19"
        app:srcCompat="@drawable/home_icon" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/description_icon"
        app:layout_constraintBottom_toBottomOf="@+id/editText17"
        app:layout_constraintEnd_toEndOf="@+id/editText17"
        app:layout_constraintStart_toStartOf="@+id/editText17"
        app:layout_constraintTop_toTopOf="@+id/editText17"
        app:srcCompat="@drawable/description_icon" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/school_icon"
        app:layout_constraintBottom_toBottomOf="@+id/editText16"
        app:layout_constraintEnd_toEndOf="@+id/editText16"
        app:layout_constraintStart_toStartOf="@+id/editText16"
        app:layout_constraintTop_toTopOf="@+id/editText16"
        app:srcCompat="@drawable/school_icon" />

    <ImageView
        android:id="@+id/imageView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/workspace_icon"
        app:layout_constraintBottom_toBottomOf="@+id/editText18"
        app:layout_constraintEnd_toEndOf="@+id/editText18"
        app:layout_constraintStart_toStartOf="@+id/editText18"
        app:layout_constraintTop_toTopOf="@+id/editText18"
        app:srcCompat="@drawable/workspace_icon" />

    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="240dp"
        android:contentDescription="@string/mobile"
        app:layout_constraintBottom_toBottomOf="@+id/editText20"
        app:layout_constraintEnd_toEndOf="@+id/editText20"
        app:layout_constraintStart_toStartOf="@+id/editText20"
        app:layout_constraintTop_toTopOf="@+id/editText20"
        app:srcCompat="@drawable/call_icon" />

    <View
        android:id="@+id/divider15"
        android:layout_width="409dp"
        android:layout_height="1dp"
        android:layout_marginTop="7dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />
</androidx.constraintlayout.widget.ConstraintLayout>