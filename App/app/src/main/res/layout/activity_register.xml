<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".register">

    <Button
        android:id="@+id/register_button2"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:background="#FFFFFF"
        android:contentDescription="@string/app_button_register"
        android:text="@string/app_button_register"
        android:textColor="#1a436e"
        android:textStyle="bold"
        app:backgroundTint="#FFFFFF"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="656dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_logo"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="187dp"
        tools:layout_editor_absoluteY="48dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="174dp"
        android:layout_height="33dp"
        android:text="@string/app_text1"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="112dp" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/app_user"
        android:inputType="text"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="203dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/app_user_email"
        android:inputType="textEmailAddress"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="274dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/app_user_confirm"
        android:inputType="textPassword"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="405dp" />

    <EditText
        android:id="@+id/editTextTextPassword3"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/app_password"
        android:inputType="textPassword"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="331dp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="350dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/lock_icon"
        tools:layout_editor_absoluteX="-98dp"
        tools:layout_editor_absoluteY="324dp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="350dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/lock_icon"
        tools:layout_editor_absoluteX="-100dp"
        tools:layout_editor_absoluteY="393dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="350dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/person_icon"
        tools:layout_editor_absoluteX="-98dp"
        tools:layout_editor_absoluteY="196dp" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="350dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/mail_icon"
        tools:layout_editor_absoluteX="-100dp"
        tools:layout_editor_absoluteY="265dp" />

</androidx.constraintlayout.widget.ConstraintLayout>