<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/FUNDO"
    tools:context=".LogInPageActivity">

    <Button
        android:id="@+id/button14"
        style="@style/Widget.Material3.Button.ElevatedButton"
        android:layout_width="196dp"
        android:layout_height="39dp"
        android:background="@color/white"
        android:backgroundTint="@color/Beige"
        android:text="@string/login"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="534dp" />

    <Button
        android:id="@+id/button9"
        style="@style/Widget.Material3.Button.ElevatedButton"
        android:layout_width="160dp"
        android:layout_height="35dp"
        android:background="@color/white"
        android:backgroundTint="@color/Beige"
        android:text="@string/Return"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="27dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="251dp"
        android:layout_height="262dp"
        app:srcCompat="@drawable/the_sitter_3"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="62dp" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/textInputEditText"
        android:layout_width="369dp"
        android:layout_height="63dp"
        android:hint="Username"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="345dp" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="369dp"
        android:layout_height="63dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="@string/PASSWORD"
        android:inputType="textVisiblePassword"
        app:layout_constraintEnd_toEndOf="@+id/textInputEditText"
        app:layout_constraintStart_toStartOf="@+id/textInputEditText"
        app:layout_constraintTop_toBottomOf="@+id/textInputEditText" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Forgot Your Password?"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="581dp" />
</androidx.constraintlayout.widget.ConstraintLayout>